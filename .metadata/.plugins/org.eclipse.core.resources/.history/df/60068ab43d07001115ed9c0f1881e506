		super(marka, gasolina, gastua, altuera, abiaduraMax);
		this.martxaKop = martxaKop;
	}

	@Override
	public String toString() {
		
		return super.toString()+"Kotxea [martxaKop=" + martxaKop + "]";
		
	}

	@Override
	public int zenbatDenboraIristeko(int distantzia) {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public boolean pasaDaiteke(float altuera) {
		// TODO Auto-generated method stub
		return false;
	}
	
	public static void main(String[] args) {
		Kotxea k1=new Kotxea("Tucson", 56, 20, (float) 1.5, 120, 5);
	}
	
	
	

}
