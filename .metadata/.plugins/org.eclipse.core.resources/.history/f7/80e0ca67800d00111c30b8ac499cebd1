package kontaktu;

public   abstract class Kontaktua implements Balidagarria { // NO SE PUEDE INSTANCIAR
	
	private boolean baliozkoa;

	public boolean isBaliozkoa() {
		return baliozkoa;
	}

	public void setBaliozkoa(boolean baliozkoa) { // USAR EL SETTER PARA LUEGO CAMBIAR LOS VALORES
		this.baliozkoa = baliozkoa;
	}

	@Override
	public String toString() {
		return "Kontaktua [baliozkoa=" + baliozkoa + "]";
	}
	
	

}
